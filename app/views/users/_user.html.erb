<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Birthdate:</strong>
    <%= user.birthdate %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Telefone:</strong>
    <%= user.telefone %>
  </p>

  <p>
    <strong>Profile:</strong>
    <%= user.profile %>
  </p>

</div>
